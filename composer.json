{
  "name": "tumichnix/laravel-telegram-logger",
  "description": "A Laravel package for logging via telegram bot",
  "keywords": [
    "laravel",
    "telegram",
    "logging",
    "log",
    "php"
  ],
  "homepage": "https://hannesdaus.de",
  "license": "MIT",
  "authors": [
    {
      "name": "Hannes Daus",
      "email": "tumichnix@gmail.com",
      "homepage": "https://hannesdaus.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "^6.5 || ^7.0",
    "illuminate/http": "^7.0 || ^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^5.0 || ^6.0",
    "phpunit/phpunit": "^8.0"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Tumichnix\\TelegramLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tumichnix\\TelegramLogger\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "support": {
    "email": "tumichnix@gmail.com",
    "issues": "https://github.com/tumichnix/laravel-telegram-logger/issues",
    "source": "https://github.com/tumichnix/laravel-telegram-logger"
  }
}
